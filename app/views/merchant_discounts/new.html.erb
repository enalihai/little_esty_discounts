<div class="feature-box">
  <h1>Create New Discount</h1><br>
  <div class="form">
    <%= render partial: "form", locals: { path:"/merchants/#{@merchant.id}/discounts", method: :post, label: "Submit",} %>
  </div>

  <div class="button-to">
    <%= button_to "Discounts Index", "/merchants/#{@merchant.id}/discounts", method: :get %>
  </div>
  <div class="button-to">
    <%= button_to "Merchant Info", "/merchants/#{@merchant.id}", method: :get %>
  </div>
</div>
